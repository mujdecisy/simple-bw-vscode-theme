	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#eeeeee"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#eeeeee"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
